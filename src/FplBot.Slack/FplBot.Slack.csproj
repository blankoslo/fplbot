<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <LangVersion>latest</LangVersion>
    <RootNamespace>FplBot.Slack</RootNamespace>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Fastenshtein" Version="1.0.0.7" />
    <PackageReference Include="AngleSharp" Version="1.0.0-alpha-844" />
    <PackageReference Include="CronBackgroundServices" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="$(AspNetVersion)" />
    <PackageReference Include="StackExchange.Redis" Version="2.2.50" />
    <PackageReference Include="Slackbot.Net.Endpoints" Version="$(SlackbotNetVersion)" />
    <PackageReference Include="Slackbot.Net.SlackClients.Http" Version="$(SlackbotNetVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Fpl.Client\Fpl.Client.csproj" />    
    <ProjectReference Include="..\FplBot.Formatting\FplBot.Formatting.csproj" />    
    <ProjectReference Include="..\FplBot.Messaging.Contracts\FplBot.Messaging.Contracts.csproj" />
    <ProjectReference Include="..\Fpl.Search\Fpl.Search.csproj" />
  </ItemGroup>
</Project>
